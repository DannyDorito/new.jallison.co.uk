  "private": true,
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "apollo3-cache-persist": "^0.15.0",
    "eslint-config-prettier": "^9.1.0",
    "graphql": "^16.9.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.81.0",
    "svgo": "^3.3.2",
    "usehooks-ts": "^3.1.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.2",
    "prettier": "3.3.3",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "typescript": "^5.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "update": "npm install --save-dev @types/react@latest @types/react-dom@latest && npm install react-router-dom && npm install react-scripts",
    "svgs": "npx @svgr/cli --typescript --out-dir src/svgs --ignore-existing src/raw-svgs",
    "outdated": "npm outdated --depth=10",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
